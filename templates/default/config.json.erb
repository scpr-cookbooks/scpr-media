{
  "port": 8000,
  "audio_dir": "/scpr/media/media/audio",
  "redirect_url": "<%= @config["redirect_url"].to_s %>",
  "prefixes": {
    "/audio": { "preroll_key": "ondemand" },
    "/podcasts": { "preroll_key": "podcast" }
  },
  <% if @config["preroll_enabled"] %>
  "preroll": {
    "path": "<%= @config["preroll"]["path"] %>",
    "server": "<%= @config["preroll"]["server"] %>"
  },
  <% end %>
  "google_analytics": {
     "property": "<%= @config["google_analytics"]["property"] %>",
     "custom_dimension": "<%= @config["google_analytics"]["custom_dimension"] %>"
  },
  "debug": false
}
